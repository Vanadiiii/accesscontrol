<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <changeSet id="2" author="imatveev">
        <createProcedure>
            do
            $init_keys$
            declare i int;
            begin
            for i in 1..10000
            loop
            insert into key values (i) on conflict do nothing;
            end loop;
            end;
            $init_keys$;
        </createProcedure>
        <createProcedure>
            do
            $init_rooms$
            declare i int;
            begin
            for i in 1..5
            loop
            insert into room values (i) on conflict do nothing;
            end loop;
            end;
            $init_rooms$;
        </createProcedure>
    </changeSet>

</databaseChangeLog>